<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
    <base-config>
        <trust-anchors>
            <!-- Trust user-installed certs (includes maui-dev-cert.cer) -->
            <certificates src="user" />
            <!-- Keep system CAs too -->
            <certificates src="system" />
        </trust-anchors>
    </base-config>
</network-security-config>